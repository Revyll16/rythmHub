<div class="container-fluid-form m-4">
  <h1>Edit Musician</h1>
  <%= simple_form_for(@musician) do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :bio %>
    <%= f.input :image_url %>
    <%= f.submit%>

    <%= link_to "Cancel", musician_path, class: "button", style: "text-decoration: none; color: white; background-color: #007BFF; padding: 8px 16px; border-radius: 4px" %>
  <% end %>
</div>
